{
	"name": "lanten-color-theme-syntax",
	"type": "light",
	"colors": {
		"editor.background": "#fffbf6",
		"editor.foreground": "#4B83CD", // 前景色
		"editor.selectionBackground": "#ffe3bf",
		"editor.inactiveSelectionBackground": "#cebeaa",
		"editor.selectionHighlightBackground": "#decdb1",
		"editor.findMatchBackground": "#24a5cb80",
		"editor.findMatchHighlightBackground": "#d7d4f060",
		"editor.findRangeHighlightBackground": "#d7d4f040",
		"editor.hoverHighlightBackground": "#add6ff",
		"editor.lineHighlightBackground": "#d7d4f030",
		"editor.rangeHighlightBackground": "#fdff00",
		"editorCursor.foreground": "#39464e",
		"editorWhitespace.foreground": "#969896",
		"editorIndentGuide.background": "#f6f2ee",
		"editor.wordHighlightBackground": "#b1d2de",
		"editor.wordHighlightStrongBackground": "#d7d4f070",
		"peekViewResult.matchHighlightBackground": "#d7d4f060",
		"peekViewEditor.matchHighlightBackground": "#f5d802",
		"activityBar.background":"#48443c73",
		"sideBar.background":"#fffbf6",
		"list.hoverBackground":"#48443c26",
		"list.inactiveSelectionBackground":"#48443c26",
		"list.activeSelectionBackground":"#48443c26",
		"editorGroup.background":"#fffbf6",
		"editorGroupHeader.tabsBackground":"#fffbf6",
		"tab.border":"#fffbf6",
		"tab.inactiveBackground":"#48443c26",
		"statusBar.background":"#ff9847"
	},
	"tokenColors": [{
			"settings": {
				"foreground": "#333333ff",
				"background": "#f5f5f5ff"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"name": "Invalid - Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#96000014"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#96000014",
				"foreground": "#660000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#50AEC6"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#ff1790" // 语言常数
			}
		},
		{
			"name": "Variables", // 变量
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#C550C6"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9320EB"
			}
		},
		{
			"name": "Classes", // 类
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FE7C00"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Numbers, Characters", // 数字，字符
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Strings", // 字符串
			"scope": "string",
			"settings": {
				"foreground": "#52BB76"
			}
		},
		{
			"name": "Strings: Escape Sequences", // 字符串：转义序列
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#3EC5CB"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Strings: Symbols", // 字符串：符号
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#12B018"
			}
		},
		{
			"name": "Punctuation", // 标点
			"scope": "punctuation",
			"settings": {
				"foreground": "#0148A6"
			}
		},
		{
			"name": "Embedded Source",
			"scope": [
				"string source",
				"text source"
			],
			"settings": {
				"background": "#EAEBE6"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#0148A6"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#FE7C00"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#C550C6"
			}
		},
		{
			"name": "HTML: Entities", // html 实体
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#89B185"
			}
		},
		{
			"name": "CSS: Selectors", // css 选择器
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#FE7C00"
			}
		},
		{
			"name": "CSS: Property Names", // CSS：属性名称
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#12B018"
			}
		},
		{
			"name": "CSS: Property Values", // CSS：属性值
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#C550C6"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"background": "#FFFFDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"background": "#96000014",
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output", // 标记：输出
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "Markup: Prompt", // 标记：提示
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#9320EB"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#F9D534"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling", // 标记造型
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline", // 标记内联
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#89B185"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"background": "#DDDDFF",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#434343"
			}
		}
	]
}